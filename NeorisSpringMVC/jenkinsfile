pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
			steps{
				echo 'dejo de compilar'
			}
			stage('sub building'){
				stage{
					echo 'hola mundo'
				}
			}
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}